{
  "name": "devlab",
  "version": "1.7.1",
  "description": "Node utility for running containerized tasks",
  "keywords": [
    "docker",
    "container",
    "test",
    "testing",
    "dev",
    "workflow",
    "task"
  ],
  "main": "index.js",
  "files": [
    "build",
    "index.js"
  ],
  "bin": {
    "lab": "index.js",
    "devlab": "index.js"
  },
  "preferGlobal": true,
  "scripts": {
    "test": "npm run lint && npm run mocha",
    "mocha": "mocha",
    "cover": "istanbul cover _mocha",
    "lint": "eslint . --fix",
    "local-install": "npm install -g .",
    "changelog": "changelog devlab all -m > CHANGELOG.md"
  },
  "homepage": "https://github.com/TechnologyAdvice/DevLab",
  "repository": {
    "type": "git",
    "url": "https://github.com/TechnologyAdvice/DevLab"
  },
  "bugs": {
    "url": "https://github.com/TechnologyAdvice/DevLab/issues"
  },
  "author": "TechnologyAdvice",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.3.0",
    "changelog": "^1.0.7",
    "eslint": "^1.5.1",
    "istanbul": "^0.4.3",
    "mocha": "^2.3.3",
    "mocha-sinon": "^1.1.4",
    "proxyquire": "^1.7.3",
    "sinon": "^1.17.0",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "bluebird": "^2.10.1",
    "chalk": "^1.1.1",
    "js-yaml": "^3.4.2",
    "lodash": "^3.10.1",
    "minimist": "^1.2.0",
    "shelljs": "^0.5.3",
    "username": "^1.0.1"
  }
}
